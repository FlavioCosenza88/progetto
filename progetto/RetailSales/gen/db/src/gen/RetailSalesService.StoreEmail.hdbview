VIEW RetailSalesService_StoreEmail AS SELECT
  T_STORE_EMAIL_0.createdAt,
  T_STORE_EMAIL_0.createdBy,
  T_STORE_EMAIL_0.modifiedAt,
  T_STORE_EMAIL_0.modifiedBy,
  T_STORE_EMAIL_0.STORE_ID,
  T_STORE_EMAIL_0.STORE,
  T_STORE_EMAIL_0.STORE_EMAIL,
  T_STORE_EMAIL_0.IS_JOB_SCHEDULED,
  T_STORE_EMAIL_0.DATE_FROM,
  T_STORE_EMAIL_0.DATE_TO
FROM T_STORE_EMAIL AS T_STORE_EMAIL_0