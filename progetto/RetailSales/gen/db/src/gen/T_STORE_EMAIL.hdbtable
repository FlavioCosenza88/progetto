COLUMN TABLE T_STORE_EMAIL (
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  STORE_ID NVARCHAR(5) NOT NULL,
  STORE NVARCHAR(35),
  STORE_EMAIL NVARCHAR(100),
  IS_JOB_SCHEDULED NVARCHAR(1),
  DATE_FROM DATE,
  DATE_TO DATE,
  PRIMARY KEY(STORE_ID)
)